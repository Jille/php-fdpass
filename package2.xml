<?xml version="1.0"?>
<package version="2.0" xmlns="http://pear.php.net/dtd/package-2.0"
    xmlns:tasks="http://pear.php.net/dtd/tasks-1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://pear.php.net/dtd/tasks-1.0
http://pear.php.net/dtd/tasks-1.0.xsd
http://pear.php.net/dtd/package-2.0
http://pear.php.net/dtd/package-2.0.xsd">

  <name>fdpass</name>
  <channel>pecl.php.net</channel>

  <summary>An extension which provides filedescriptor passing</summary>

  <description>
		With fdpass you can pass filedescriptors between UNIX processes over AF_UNIX-sockets.
  </description>

  <lead>
    <name>Jille Timmermans</name>
    <user>unknown</user>
    <email>jille@quis.cx</email>
    <active>yes</active>
  </lead>

  <date>2011-04-25</date>
  <version>
    <release>0.1</release>
    <api>0.1</api>
  </version>
  <stability>
    <release>beta</release>
    <api>beta</api>
  </stability>

  <license filesource="LICENSE">BSD</license>

  <notes>
First working version
  </notes>

  <contents>
    <dir name="/">
      <file role='doc' name='EXPERIMENTAL'/>
      <file role='doc' name='CREDITS'/>
      <file role='src' name='config.m4'/>
      <file role='src' name='fdpass.dsp'/>
      <file role='src' name='config.w32'/>
      <file role='src' name='fdpass.c'/>
      <file role='src' name='php_fdpass.h'/>
      <dir name="tests">
        <file role='test' name='fdpass_send.phpt'/>
        <file role='test' name='fdpass_recv.phpt'/>
      </dir>
    </dir>
  </contents>

  <dependencies>
    <required>
      <php>
        <min>5.0.0</min>
      </php>
      <pearinstaller>
        <min>1.4.0a1</min>
      </pearinstaller>
    </required>
  </dependencies>

  <providesextension>fdpass</providesextension>

  <extsrcrelease/>

</package>
